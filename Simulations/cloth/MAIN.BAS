DIM xcolor AS _UNSIGNED LONG, x AS _UNSIGNED _INTEGER64
DIM y AS _UNSIGNED _INTEGER64
SCREEN _NEWIMAGE(640, 480, 32)
DO UNTIL INKEY$ <> ""
    FOR frame% = 1 TO 150
        HANDLE = _NEWIMAGE(640, 480, 32)
        _DEST HANDLE
        FOR y = 480 TO 0 STEP -1
            FOR x = 0 TO 640
                READ xcolor: PSET (x, y), xcolor
            NEXT x
        NEXT y
        _DEST 0
        _PUTIMAGE (0, 0)-(640, 480), HANDLE, 0
        _FREEIMAGE HANDLE
        _LIMIT 15
    NEXT frame%
RESTORE: LOOP
SYSTEM
